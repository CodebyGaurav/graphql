type Mutation{
    createBook(book:BookInput):Book
}

input BookInput {
    title:String
    description:String
    author:String
    price:Float
    pages:Int
}

type Query{
    allBooks:[Book]
    getBookById(bookId:Int):Book
}

type Book{
    id:ID!
    title:String
    description:String
    author:String
    price:Float
    pages:Int
}